<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.yhl.laoyou.modules.sys.dao.APPVersionDao">


	<insert id="insert">
		INSERT INTO`app_version` (`version`, `Introduction`, `dowloadURL`) VALUES (#{version}, #{introduction}, #{dowloadURL})
	</insert>



	<select id="get" resultType="com.yhl.laoyou.common.dto.APPVersionDTO">
		SELECT * FROM`app_version` where source=#{source}
	</select>



</mapper>